{
  "manifest_version": 1,

  "name": "The Failing NYTimes",
  "description": "Replace every instance of the The NYTimes to the failing NYTimes.",
  "version": "1.0",
  "icons": { 
    "16": "icon-16.png",
    "48": "icon-48.png", 
    "128": "icon-128.png"
  },
  "browser_action": {
    "default_icon": "icon-48.png",
    "default_popup": "switcher.html"
  },
  "content_scripts": [
    {
      "matches":[
        "http://*/*",
        "https://*/*"
      ],
      "js": ["switcher.js"],
      "run_at": "document_end"
    }
  ]
}